<?xml version="1.0" encoding="utf-8"?>
<!-- To override the default close button change the name of this file to
     card_scan_close_button_button.xml and set the states appropriately. Currently
     we flipped pressed and non-pressed images just to show that it works. -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@drawable/ic_close_white" />
    <item android:state_pressed="false" android:drawable="@drawable/close_white_pressed" />
</selector>